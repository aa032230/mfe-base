{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false, 
  "singleQuote": true, 
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "ignore",
  "vueIndentScriptAndStyle": false,
  "endOfLine": "lf"
}